		"node": "20"
	},
	"dependencies": {
		"firebase-admin": "^12.0.0",
		"firebase-functions": "^6.1.0",
		"puppeteer": "^23.6.1"
	},
	"devDependencies": {
		"typescript": "^5.6.3"
	},
	"private": true
}
